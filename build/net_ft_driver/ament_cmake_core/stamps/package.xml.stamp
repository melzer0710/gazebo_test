<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>net_ft_driver</name>
  <version>0.0.0</version>
  <description>ROS2 driver for Net F/T sensors (ATI, ATI Axia, OnRobot)</description>

  <maintainer email="gbartyzel@hotmail.com">Grzegorz Bartyzel</maintainer>

  <license>Apache-2.0</license>

  <author>Grzegorz Bartyzel</author>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>asio</depend>
  <depend>asio_cmake_module</depend>
  <depend>hardware_interface</depend>
  <depend>pluginlib</depend>
  <depend>rclcpp</depend>
  <depend>rclcpp_lifecycle</depend>
  <depend>tinyxml2</depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
